{
  "name": "stitch-me-up",
  "version": "0.1.3",
  "description": "Stitch microservices together with docker & docker-compose.",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "test-unit": "mocha --require co-mocha ./test/unit/**/*.spec.js",
    "test-functional": "mocha --require co-mocha ./test/functional/**/*.spec.js",
    "test": "npm run lint && npm run test-unit && npm run test-functional",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "bin": {
    "stitch": "./bin/stitch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diosmosis/stitch-me-up.git"
  },
  "keywords": [
    "docker",
    "microservice",
    "docker-compose",
    "local",
    "deployment"
  ],
  "preferGlobal": true,
  "author": "diosmosis",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/diosmosis/stitch-me-up/issues"
  },
  "homepage": "https://github.com/diosmosis/stitch-me-up#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "co-mocha": "^1.1.2",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.9.2",
    "mocha": "^2.5.3",
    "mock-require": "^1.3.0"
  },
  "dependencies": {
    "co": "^4.6.0",
    "co-exec": "^1.1.0",
    "fs-utils": "^0.7.0",
    "js-yaml": "^3.6.1",
    "lodash": "^4.13.1",
    "minimist": "^1.2.0",
    "passthru": "git+https://github.com/diosmosis/node-passthru.git#argv-split",
    "request-promise": "^3.0.0",
    "valid-url": "^1.0.9",
    "snyk": "^1.189.0"
  },
  "snyk": true
}
